{
  "name": "intro-to-nodejs-npm",
  "version": "1.2.0-beta.8",
  "description": "npm into package",
  "main": "1-built-ins.js",
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "prestart": "babel-node ./buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch ",
    "open:src": "babel-node ./buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Benjamin Huang",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "config-chain": "^1.1.11",
    "eslint": "^4.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "growl": "^1.10.2",
    "grunt": "^1.0.1",
    "inline-source-map": "^0.6.2",
    "jsdom": "^11.1.0",
    "jshint": "^2.9.5",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "underscore": "^1.8.3",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminbhuang/IntroToNodejs.git"
  },
  "keywords": [
    "intro",
    "to",
    "nodejs",
    "npm"
  ],
  "bugs": {
    "url": "https://github.com/benjaminbhuang/IntroToNodejs/issues"
  },
  "homepage": "https://github.com/benjaminbhuang/IntroToNodejs#readme"
}
